<!--
	Nonfunctional hosts in *.skuba.ch:
		- sr.skuba.ch (r)

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Skuba.ch">
	<target host="skuba.ch" />
	<target host="www.skuba.ch" />

	<rule from="^http:" to="https:" />
</ruleset>
