<!--
	For other ricardo coverage, see Ricardo.ch.xml.

	Mismatch:
		- ^ricardoshops.ch
-->
<ruleset name="ricardoshops.ch">

	<target host="ricardoshops.ch" />
	<target host="www.ricardoshops.ch" />


	<securecookie host=".+" name=".+" />


	<rule from="^http://ricardoshops\.ch/"
		to="https://www.ricardoshops.ch/" />
	<rule from="^http:"
		to="https:" />

</ruleset>
